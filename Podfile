# platform :ios, '9.0'

target 'Swiper_iOS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Swiper_iOS
  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'Firebase/Auth'
  pod 'FirebaseUI/Auth'
  pod 'FirebaseUI/Facebook'
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'
  pod ‘Parse/FacebookUtils’

end
